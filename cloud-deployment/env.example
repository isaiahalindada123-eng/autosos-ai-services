# AutoSOS Cloud Deployment Environment Variables

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key

# Redis Configuration
REDIS_URL=redis://redis:6379

# Service URLs (for internal communication)
FACENET_SERVICE_URL=http://facenet-service:8001
YOLO_SERVICE_URL=http://yolo-service:8002
OLLAMA_SERVICE_URL=http://ollama-service:11434

# Model Cache Directories
MODEL_CACHE_DIR=/app/models

# Security
JWT_SECRET_KEY=your-jwt-secret-key
API_KEY=your-api-key

# Monitoring
PROMETHEUS_ENABLED=true
GRAFANA_ADMIN_PASSWORD=admin

# SSL Configuration (for production)
SSL_CERT_PATH=/etc/nginx/ssl/cert.pem
SSL_KEY_PATH=/etc/nginx/ssl/key.pem

# Resource Limits
FACENET_MEMORY_LIMIT=2G
YOLO_MEMORY_LIMIT=4G
OLLAMA_MEMORY_LIMIT=8G

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json
